  "license": "MIT",
  "scripts": {
    "migrate:database": "npx prisma migrate dev --name init --schema=./libs/database/src/prisma/schema.prisma",
    "database:generate": "npx prisma generate --schema=./libs/database/src/prisma/schema.prisma",
    "database:push": "npx prisma db push --schema=./libs/database/src/prisma/schema.prisma",
    "backend": "npx nx run backend:serve",
    "frontend": "npx nx run frontend:dev",
    "all": "concurrently -k \"npm run backend\" \"npm run frontend\" ",
    "all:sequential": "npx nx run-many --targets=backend:serve,frontend:dev --parallel=false",
    "build:app": "nest build",
    "start:prod": "node dist/main.js",
    "build:azure": "npx nx run functions:build --prod"
  },
